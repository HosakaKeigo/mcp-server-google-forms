{
  "name": "mcp-server-google-forms",
  "version": "1.0.0",
  "description": "MCP server for Google Forms API",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsc && node build/index.js",
    "lint": "biome lint ./src",
