    "@modelcontextprotocol/sdk": "^1.11.4",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.15.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.8.2",
    "typescript": "^5.2.2"
  }
}